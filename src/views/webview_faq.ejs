        img{
            width: 120px !important;
        }
        .accordion-header
        {
            background: #f8f9fa !important;
        }
        .accordion-button:not(.collapsed) 
        {
            background-color: #0c63e4;
        }
        .accordion-button
        {
            color: #0c63e4;
            background-color: transparent !important;
        }
